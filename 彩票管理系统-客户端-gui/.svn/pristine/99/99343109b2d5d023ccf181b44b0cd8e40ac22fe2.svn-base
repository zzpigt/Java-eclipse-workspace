    private void jCommitButtonActionPerformed(java.awt.event.ActionEvent evt) {                                              
    	// 预购提交按钮
    	showNum = "  红球：  "+this.jRB1Field.getText().trim()+" "
    				+this.jRB2Field.getText().trim()+" "
    				+this.jRB3Field.getText().trim()+" "
    				+this.jRB4Field.getText().trim()+" "
    				+this.jRB5Field.getText().trim()+" "
    				+this.jRB6Field.getText().trim()+"\t蓝球:  "
    				+this.jBBField.getText().trim()+"\t"+
    				this.jCountField.getText().trim()+"注";
    	buyNum = this.jRB1Field.getText().trim()+"-"+
    			this.jRB2Field.getText().trim()+"-"+
    			this.jRB3Field.getText().trim()+"-"+
    			this.jRB4Field.getText().trim()+"-"+
    			this.jRB5Field.getText().trim()+"-"+
    			this.jRB6Field.getText().trim()+":"+
    			this.jBBField.getText().trim();
    	System.out.println(buyNum);
    	if(this.jCountField.getText().equals("")) {
    		buyCount = 1;
    	}else {
    		buyCount = Integer.valueOf(this.jCountField.getText().trim());
    	}
    	if (buyCount > 5 && buyCount < 0) {
    		JOptionPane.showMessageDialog(this, "请输入正确的注数：");
			this.jCountField.setText("");
			//置空
			buyNum = null;
			return;
		}
    	reset();
    	this.jCommitField.setText(showNum);
    }                                             

    private void jBuyButtonActionPerformed(java.awt.event.ActionEvent evt) throws IOException {                                           
    	// 下注按钮
    	if(buyNum != null) {
    		
    		MyRequest request = new MyRequest(MyRequest.TYPE_BET);
			request.getUmap().put(MyRequest.MEG_USERNAME, uName);
			request.getUmap().put(MyRequest.MEG_LEVEL, "彩民");
			request.getUmap().put(MyRequest.LOTTERY_SELECTNUM, buyNum);
			request.getUmap().put(MyRequest.LOTTERY_BUYCOUNT, String.valueOf(buyCount));
			request.getUmap().put(MyRequest.LOTTERY_TIME, ltime);
			// 包装好了，就发送过去，序列化
			oos.writeObject(request);
			oos.flush();

			synchronized (ClientDemo.class) {
				if (ClientDemo.lastResponse == null) {
					try {
						ClientDemo.class.wait();
					} catch (InterruptedException e) {
						e.printStackTrace();
					}
				}

				// 读到了消息
				lastResponse = ClientDemo.lastResponse;
				ClientDemo.lastResponse = null;
				System.out.println(lastResponse.getrMeg().get(MyResponse.MEG_CONTENT));
				// 判断是否大于5注，
				System.out.println(lastResponse.isSuccess());
				if(lastResponse.isSuccess()) {
					JOptionPane.showMessageDialog(this, "购买成功！！");
				}else {
					JOptionPane.showMessageDialog(this, "购买该彩票号码已经超过5注，或您的余额不足，请重新购买");
					reset();
				}
				//本期已购
				//更新余额
				uMoney = lastResponse.getrMeg().get(MyResponse.MEG_MONEY);
				sellNum = lastResponse.getrMeg().get(MyResponse.LETTERY_SELLNUM);
				poolMoney = lastResponse.getrMeg().get(MyResponse.LETTERY_POOLMONEY);
				
//				this.jMoneyLabel.setText(uMoney);
				//更新余额
				ClientDemo.class.notify();
			}
   	
    	}else {
    		JOptionPane.showMessageDialog(this, "尚未添加彩票，无法下注");
    		return;
    	}
    	goBuyLottFrame();
    }                                          
    
    private void goBuyLottFrame() {
		// TODO Auto-generated method stub
    	BuyLottFrame blf = new BuyLottFrame(oos,lastResponse,uName,uMoney,sellNum,poolMoney);
        blf.setVisible(true);
        this.dispose();
	}
    
	private void reset() {
		// 重置
    	this.jRB1Field.setText("");
		this.jRB2Field.setText("");
		this.jRB3Field.setText("");
