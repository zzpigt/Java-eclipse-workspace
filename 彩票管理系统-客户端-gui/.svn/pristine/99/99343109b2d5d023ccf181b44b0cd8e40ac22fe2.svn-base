    private void jRefreshButtonActionPerformed(java.awt.event.ActionEvent evt) throws IOException {                                               
    	// 刷新按钮
    	this.jTextArea1.setText("以下为您本期已购号码：");
    	
    	MyRequest request_flush = new MyRequest(MyRequest.TYPE_BET_FLUSH);
		request_flush.getUmap().put(MyRequest.MEG_USERNAME, uName);
		request_flush.getUmap().put(MyRequest.MEG_LEVEL, "彩民");
		request_flush.getUmap().put(MyRequest.LOTTERY_TIME, ltime);
		// 包装好了，就发送过去，序列化
		oos.writeObject(request_flush);
		oos.flush();
		
		synchronized (ClientDemo.class) {
			if (ClientDemo.lastResponse == null) {
				try {
					ClientDemo.class.wait();
				} catch (InterruptedException e) {
					e.printStackTrace();
				}
			}

			// 读到了消息
			MyResponse lastResponse = ClientDemo.lastResponse;
			ClientDemo.lastResponse = null;
			//本期已购
			List<String> historyList = lastResponse.getHistoryList();
			for (String record : historyList) {
				String text = this.jTextArea1.getText();
				this.jTextArea1.setText(text+"\n"+record);
			}
			//更新余额
//			uMoney = lastResponse.getrMeg().get(MyResponse.MEG_MONEY);
			
			
//			this.jMoneyLabel.setText(uMoney);
			//更新余额
			ClientDemo.class.notify();
		}
    }                                              

